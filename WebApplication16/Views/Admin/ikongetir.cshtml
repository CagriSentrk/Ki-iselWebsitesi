@model WebApplication16.Models.Siniflar.ikonlar
@{
    ViewBag.Title = "ikongetir";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>ikon güncelle</h2>
<br />
@using (Html.BeginForm("ikonguncelle", "admin", FormMethod.Post))
{
    @Html.LabelFor(x => x.id)
    @Html.TextBoxFor(x => x.id, new { @class = "form-control", @readonly = "readonly" })
    <br />
    @Html.LabelFor(x => x.ikonad)
    @Html.TextBoxFor(x => x.ikonad, new { @class = "form-control" })
    <br />
    @Html.LabelFor(x => x.link)
    @Html.TextBoxFor(x => x.link, new { @class = "form-control" })
    <br />
    <button class="btn btn-success"> güncelle</button>

}


